package com.Maven_Class;

public class Class_Pojo1 {

	public static void main(String[] args) {
		Class_Pojo a=new Class_Pojo();
		a.setName("ram");
		a.setLastname('R');
		a.setSalary(30000);
		System.out.println("name:"+a.getName());
		System.out.println("last name:"+a.getLastname());
		System.out.println("salary:"+a.getSalary());
		
	}

}
